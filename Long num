7)). #include <stdio.h>

int main() {
    unsigned long long num;
    int count = 0;

    scanf("%llu", &num);

    if (num >= 1 && num <= 10000000) {
        unsigned long long temp = num;
        while (temp > 0) {
            count++;
            temp /= 10;
        }
        printf("The count of the given integer is: %d\n", count);
    } else {
        printf("Enter a Valid Input\n");
    }

    return 0;
}
